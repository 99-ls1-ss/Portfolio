
@{
    ViewBag.Title = "My Blog";
}


<section id="blog-posts" class="section colored-wrapper">
    <div class="container">
        <h2>@ViewBag.Title</h2>
        <h3>@ViewBag.Message</h3>
        <div class="wrapper-2">
            <h5 class="subpage-title">
                <span>Latest Blog Posts</span>
                <!-- Controls -->
                <span id="blog-posts-next" class="next black"><i class="icon-angle-right"></i></span>
                <span id="blog-posts-prev" class="prev black"><i class="icon-angle-left"></i></span>
            </h5>
            <div class="row">
                <div id="caroufredsel-blog-posts-container">
                    <div class="col-xs-12 col-sm-6 col-md-4">
                        <article class="post post-2">
                            <div class="post-thumb">
                                <a href="~/Content/images/blog/photo (16).jpg"><img src="~/Content/images/blog/photo (16).jpg" class="img-responsive" alt="Responsive image"></a>
                            </div>
                            <!-- /.post-thumb -->
                            <div class="post-content">
                                <h5 class="post-title"><a href="post-1.html">The Ultimate Guide to Adjustment Layers – Vibrance</a></h5>
                            </div>
                            <!-- /.post-content -->
                            <div class="post-meta">
                                <ul class="meta-list">
                                    <li>
                                        <span>Posted on</span>
                                        <span class="black">10 July 2013</span>
                                    </li>
                                </ul>
                            </div>
                            <!-- /.post-meta -->
                        </article>
                        <!-- /.post -->
                    </div>
                    <!-- /.col-md-4 -->
                    <div class="col-xs-12 col-sm-6 col-md-4">
                        <article class="post post-2">
                            <div class="post-slider">
                                <div id="carousel-example-generic" class="carousel slide">
                                    <!-- Wrapper for slides -->
                                    <div class="carousel-inner">
                                        <div class="item active">
                                            <img src="~/Content/images/blog/photo (10).jpg" class="img-responsive" alt="Carousel Image">
                                        </div>
                                        <div class="item">
                                            <img src="~/Content/images/blog/photo (9).jpg" class="img-responsive" alt="Carousel Image">
                                        </div>
                                        <div class="item">
                                            <img src="~/Content/images/blog/photo (13).jpg" class="img-responsive" alt="Carousel Image">
                                        </div>
                                    </div>
                                    <!-- Controls -->
                                    <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
                                        <span class="glyphicon glyphicon-chevron-left"></span>
                                    </a>
                                    <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
                                        <span class="glyphicon glyphicon-chevron-right"></span>
                                    </a>
                                </div>
                            </div>
                            <!-- /.post-slider -->
                            <div class="post-content">
                                <h5 class="post-title"><a href="post-1.html">Complete Guide to What’s New in Adobe Photoshop 14.1</a></h5>
                            </div>
                            <!-- /.post-content -->
                            <div class="post-meta">
                                <ul class="meta-list">
                                    <li>
                                        <span>Posted on</span>
                                        <span class="black">15 July 2013</span>
                                    </li>
                                </ul>
                            </div>
                            <!-- /.post-meta -->
                        </article>
                        <!-- /.post -->
                    </div>
                    <!-- /.col-md-4 -->
                    <div class="col-xs-12 col-sm-6 col-md-4">
                        <article class="post post-2">
                            <div class="responsive-video-wrapper">
                                <iframe src="//player.vimeo.com/video/25708134?title=0&amp;byline=0&amp;portrait=0&amp;color=ffffff" width="800" height="400" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
                            </div>
                            <!-- /.responsive-video-wrapper -->
                            <div class="post-content">
                                <h5 class="post-title"><a href="post-1.html">How to Integrate Lightroom Into Your Photoshop Workflow</a></h5>
                            </div>
                            <!-- /.post-content -->
                            <div class="post-meta">
                                <ul class="meta-list">
                                    <li>
                                        <span>Posted on</span>
                                        <span class="black">20 July 2013</span>
                                    </li>
                                </ul>
                            </div>
                            <!-- /.post-meta -->
                        </article>
                        <!-- /.post -->
                    </div>
                    <!-- /.col-md-4 -->
                    <div class="col-xs-12 col-sm-6 col-md-4">
                        <article class="post post-2">
                            <div class="post-thumb">
                                <a href="~/Content/images/blog/photo (3).jpg"><img src="~/Content/images/blog/photo (3).jpg" class="img-responsive" alt="Responsive image"></a>
                            </div>
                            <!-- /.post-thumb -->
                            <div class="post-content">
                                <h5 class="post-title"><a href="post-1.html">The Ultimate Guide to Adjustment Layers – Vibrance</a></h5>
                            </div>
                            <!-- /.post-content -->
                            <div class="post-meta">
                                <ul class="meta-list">
                                    <li>
                                        <span>Posted on</span>
                                        <span class="black">25 July 2013</span>
                                    </li>
                                </ul>
                            </div>
                            <!-- /.post-meta -->
                        </article>
                        <!-- /.post -->
                    </div>
                    <!-- /.col-md-4 -->
                    <div class="col-xs-12 col-sm-6 col-md-4">
                        <article class="post post-2">
                            <div class="post-slider">
                                <div id="carousel-example-generic" class="carousel slide">
                                    <!-- Wrapper for slides -->
                                    <div class="carousel-inner">
                                        <div class="item active">
                                            <img src="~/Content/images/blog/photo (13).jpg" class="img-responsive" alt="Carousel Image">
                                        </div>
                                        <div class="item">
                                            <img src="~/Content/images/blog/photo (9).jpg" class="img-responsive" alt="Carousel Image">
                                        </div>
                                        <div class="item">
                                            <img src="~/Content/images/blog/photo (10).jpg" class="img-responsive" alt="Carousel Image">
                                        </div>
                                    </div>
                                    <!-- Controls -->
                                    <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
                                        <span class="glyphicon glyphicon-chevron-left"></span>
                                    </a>
                                    <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
                                        <span class="glyphicon glyphicon-chevron-right"></span>
                                    </a>
                                </div>
                            </div>
                            <!-- /.post-slider -->
                            <div class="post-content">
                                <h5 class="post-title"><a href="post-1.html">Complete Guide to What’s New in Adobe Photoshop 14.1</a></h5>
                            </div>
                            <!-- /.post-content -->
                            <div class="post-meta">
                                <ul class="meta-list">
                                    <li>
                                        <span>Posted on</span>
                                        <span class="black">30 July 2013</span>
                                    </li>
                                </ul>
                            </div>
                            <!-- /.post-meta -->
                        </article>
                        <!-- /.post -->
                    </div>
                    <!-- /.col-md-4 -->
                </div>
                <!-- /#caroufredsel-blog-posts-container -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.wrapper-2 -->
    </div>
    <!-- /.container -->
</section>
<!-- /#blog-posts -->

